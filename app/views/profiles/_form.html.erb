<%= form_for(@profile) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :tel %>
  <%= f.text_field :tel %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :postcode %>
  <%= f.text_field :postcode %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
