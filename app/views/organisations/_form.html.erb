<%= form_for(@organisation) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :slug %>
  <%= f.text_field :slug %>

  <%= f.label :organisation_type %>
  <%= f.text_field :organisation_type %>

  <%= f.label :searchable %>
  <%= f.check_box :searchable %>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
